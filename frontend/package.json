{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite && npm run watch-routes",
    "build": "npm run generate-routes && tsc -b && vite build && npm run generate-routes",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "biome format --write ./src",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "generate-client": "orval",
    "generate-routes": "tsr generate",
    "watch-routes": "tsr watch"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "biome format --write",
      "git add"
    ]
  },
  "dependencies": {
