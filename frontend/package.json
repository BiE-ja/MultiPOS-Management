    "@mantine/code-highlight": "^8.2.7",
    "@mantine/core": "^8.2.7",
    "@mantine/dates": "^8.2.7",
    "@mantine/dropzone": "^8.2.7",
    "@mantine/form": "^8.2.7",
    "@mantine/hooks": "^8.2.7",
